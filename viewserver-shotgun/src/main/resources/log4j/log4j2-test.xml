<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
    <appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] [%X{userName}] %-5level %logger{36} - %msg%n" />
        </Console>
    </appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="Console" />
        </Root>
        <Logger name="io.grpc.netty" level="info"/>
        <Logger name="io.netty" level="info"/>
        <Logger name="io.netty.handler.codec.http.websocketx" level="info"/>
        <Logger name="io.viewserver.messages.PoolableMessage" level="info"/>
        <Logger name="io.viewserver.messages.PoolableMessage" level="info"/>
        <Logger name="io.viewserver.operators.sortxx" level="trace"/>
    </Loggers>
</configuration>