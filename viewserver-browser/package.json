{
  "name": "enhanced-electron-react-boilerplate",
  "version": "0.6.0",
  "description": "Modern and somewhat opinionated react+electron+webpack boilerplate",
  "author": "Phillip Barbiero",
  "homepage": "https://github.com/pbarbiero/enhanced-electron-react-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/pbarbiero/enhanced-electron-react-boilerplate.git"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "dev": "webpack-dev-server --host 0.0.0.0 --config=./webpack.dev.config.js",
    "prod": "webpack --progress --config webpack.build.config.js && electron --noDevServer .",
    "build": "webpack --progress --config webpack.build.config.js",
    "package": "webpack --config webpack.build.config.js",
    "postpackage": "electron-packager ./ --out=./builds",
    "scaffold": "node ./scaffold/scaffold.js",
    "postinstall": "cd ./scaffold && npm install"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0-beta.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "react-spinners": "^0.2.5",
    "style-loader": "^0.19.0",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.4.5",
    "seamless-immutable": "^7.1.2",
    "redux-seamless-immutable": "^0.3.2",
    "rxjs": "^5.5.2",
    "redux-thunk": "^2.2.0",
    "classnames": "^2.2.5",
    "electron-devtools-installer": "^2.2.0",
    "moment": "^2.19.1",
    "query-string": "^5.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.2",
    "protobufjs": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babili-webpack-plugin": "^0.1.2",
    "connected-react-router": "^4.0.0-beta.4",
    "css-loader": "^0.26.2",
    "deep-equal": "^1.0.1",
    "electron": "^1.7.8",
    "electron-json-config": "^1.4.0",
    "electron-packager": "^9.1.0",
    "yup": "^0.23.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3"
  }
}
